<editors>

    <!-- Boolean -->
    <editor id="bool">
        <range uom="2" subset="0,1" />
    </editor>

    <!-- Log Debug Mode -->
    <editor id="DEBUGMODE">
      <range uom="25" subset="0,10,20,30,40,50" nls="CDM" />
    </editor>

    <!-- Pushover Priority -->
    <editor id="PO_PRIORITY">
      <range uom="25" subset="0-4" nls="POVP" />
    </editor>

    <!-- Pushover Priority -->
    <editor id="PO_FORMAT">
      <range uom="25" subset="0-2" nls="POVF" />
    </editor>

    <!-- Pushover Retry -->
    <editor id="PO_RETRY">
      <range uom="56" subset="30-10800" />
    </editor>

    <!-- Pushover Expire -->
    <editor id="PO_EXPIRE">
      <range uom="56" subset="60-10800" />
    </editor>

    <!-- Pusover Errors -->
    <editor id="PO_ERR">
      <range uom="25" subset="0-5" nls="POVE" />
    </editor>

    <!-- Assistant Relay User -->
    <editor id="ASR_USER">
      <range uom="25" subset="0-1" nls="ASRU" />
    </editor>

    <!-- Pushover Priority -->
    <editor id="NFY_PRIORITY">
      <range uom="25" subset="0-4" nls="NFYP" />
    </editor>


</editors>
